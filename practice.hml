<body>
<script>
var word = "";
var texts = [];
var letters = "abcdefghijklmnopqrstuvwxyz";
var l = letters.length;
var textsNum = 10;
for (var v = 0; v < textsNum; v++) {
  for (var i = 0; i < l; i++) {
    word += letters[Math.floor(Math.random() * l)];
  }
  texts.push(word);
  word = '';
}
</script>
</body>
